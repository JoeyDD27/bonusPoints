{
  "manifest_version": 3,
  "name": "bonusPoints",
  "version": "1.0",
  "description": "Virtual currency system: Daily check-in for money, trade with others",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://api.notion.com/"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}